<HTML>
	<HEAD>
		<STYLE type='text/css'>
			th, td{
				width:100px;
			}
		</STYLE>
	</HEAD>
	<BODY>
	<!-- Tableau contenant toutes les données voulues -->
		<TABLE border="1px solid blue">
			<!-- Récupération des données titres -->
			{% for Name in listColnames %}
				<th>{{Name}}</th>
			{% endfor %}
			<th>Modifier</th>
                        <th>Supprimer</th>
			{% for Tableau in listEntities %}
			<tr>
				<!-- Récupération du contenu du tableau à importer -->
				{% for Contenu in Tableau %}
					<td align='right'>{{Contenu}}</td>
				{% endfor %}
                                <td align='center'><a href="{{ path('sym16_simple_stock_modifier') }}">Modifier</a></td>
                                <td align='center'><a href="{{ path('sym16_simple_stock_supprimer') }}">Supprimer</a></td>
			</tr>
			{% endfor %}
		</TABLE>
	</BODY>
</HTML>
